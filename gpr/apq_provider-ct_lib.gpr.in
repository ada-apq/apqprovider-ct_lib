with "kow_framework_setup.gpr";

with "apq.gpr";
with "apq-ct_lib.gpr";
with "apq_provider.gpr";

project APQ_Provider.ct_lib is

	version	:= APQ_Provider.Version; 
	
	
	
	for Source_Dirs use ( $include_prefix );
	for Library_Dir use $lib_prefix;
	for Library_Name use "apq_provider-ct_lib";
	for Library_Version use "libapq_provider-ct_lib.so." & Version;
	for Library_Kind use kow_framework_Setup'Library_Kind;
	for Externally_Built use "true";

	
	
	package Compiler is
		for Default_Switches ("ada") use ("-O2", "-gnat05");
	end Compiler;

end APQ_Provider.ct_lib;

